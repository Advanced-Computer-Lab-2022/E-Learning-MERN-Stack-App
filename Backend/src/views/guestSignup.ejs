<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>User Sign Up</title>
  </head>
  <body>
    <form>
      <label for="guestFnameInput">First Name: </label><br />
      <input type="text" id="guestFnameInput" name="guestFnameInput" /><br />
      <label for="guestLnameInput">Last Name: </label><br />
      <input type="text" id="guestLnameInput" name="guestLnameInput" /><br />
      <label for="guestUpEmailInput">Email: </label><br />
      <input
        type="text"
        id="guestUpEmailInput"
        name="guestUpEmailInput"
      /><br />
      <label for="guestUpPasswordInput">Password: </label><br />
      <input
        type="password"
        id="guestUpPasswordInput"
        name="guestUpPasswordInput"
      /><br />
      <label for="guestUpGenderInput">Gender: </label><br />
      <input
        type="text"
        id="guestUpGenderInput"
        name="guestUpGenderInput"
      /><br />

      <br />
      <input type="submit" id="guestSignup" value="Sign Up" />
    </form>
    <script src="./js/guestSignupHandler.js"></script>
  </body>
</html>
